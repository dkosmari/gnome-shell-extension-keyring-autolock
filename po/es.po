#: extension.js:55
msgid "Settings..."
msgstr "Ajustes..."

#: extension.js:59
msgid "Lock the keyring now"
msgstr "Bloquea el llavero ahora"

#: extension.js:111
msgid "Locked:"
msgstr "Bloqueado:"

#: prefs.js:110 prefs.js:118
msgid "Ignored collections"
msgstr "Colecciones ignoradas"

#: prefs.js:119
msgid "Ignored collections will never be locked by this extensions."
msgstr "Estas extensiones nunca bloquearán las colecciones ignoradas."

#: prefs.js:210
msgid "General settings"
msgstr "Configuración general"

#: prefs.js:218
msgid "Indicator"
msgstr "Indicador"

#: prefs.js:224
msgid "Hide indicator if locked"
msgstr "Ocultar indicador si está bloqueado"

#: prefs.js:225
msgid "Keep the indicator icon hidden while the keyring is locked."
msgstr "Mantenga oculto el icono indicador mientras el llavero esté bloqueado."

#: prefs.js:239
msgid "Timers"
msgstr "Temporizadores"

#: prefs.js:245
msgid "Check interval (seconds)"
msgstr "Intervalo de comprobación (segundos)"

#: prefs.js:246
msgid "The keyring is checked periodically, to detect if it is unlocked."
msgstr ""
"El llavero se revisa periódicamente, para detectar si está desbloqueado."

#: prefs.js:267
msgid "Lock delay (seconds)"
msgstr "Retardo de bloqueo (segundos)"

#: prefs.js:268
msgid "How long the keyring is allowed to stay unlocked."
msgstr "Cuánto tiempo se permite que el llavero permanezca desbloqueado."
